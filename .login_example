##############################################################################
# Example .cshrc file
##############################################################################
umask 022
if ($?prompt) then
  setenv TERM vt100
  setenv EDITOR /usr/ucb/vi
  set cdpath = (.. ../.. ~)
  set history=200 
  set savehist=50
  set ignoreeof
  set filec
  set prompt =  "`hostname -s`% "
  alias h 'history -r | more'
  alias ls 'ls -F'
  alias ll 'ls -al'
  alias rl 'rlogin'
endif

setenv PRINTER ps1
setenv MANPATH "/usr/man:/source/local/man" 

set path=($path /usr/local/bin . $HOME $HOME/bin)
